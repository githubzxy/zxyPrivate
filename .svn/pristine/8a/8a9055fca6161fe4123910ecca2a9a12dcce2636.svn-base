@CHARSET "UTF-8";
html, body, div, dl, dt, dd, ul, ol, li, h1, h2, h3, h4, h5, h6, pre, code, form, fieldset, legend, input, textarea, p, blockquote, th, td{margin:0;padding:0;}
table{border-collapse:collapse;border-spacing:0;}
fieldset, img{border:0;}
img{vertical-align:top; border:none;}
address, caption, cite, code, dfn, em, strong, th, var{font-style:normal;font-weight:400;}
li{list-style-type:none;}
body{width:100%;font:12px/1.5 "Microsoft YaHei","inherit","Helvetica Neue","Hiragino Sans GB",helvetica,arial,sans-serif;overflow-x:hidden;background:#F6F6F6;color:#333;}
/*删除谷歌浏览器input焦点样式*/
*:focus{outline: none;}
textarea{resize:none;}
/*删除火狐下按钮默认样式*/
input[type="reset"]::-moz-focus-inner,
input[type="button"]::-moz-focus-inner,
input[type="submit"]::-moz-focus-inner,
input[type="file"] > input[type="button"]::-moz-focus-inner{border:none; padding:0;}
input::-ms-clear{display:none;}
a:focus, *:focus{noFocusLine: expression(this.onFocus=this.blur());}
.clear{clear: both;}
a, object{outline:none;}
a{cursor:pointer;color:#444;-webkit-transition: all .3s ease-in-out;
-moz-transition: all .3s ease-in-out;
transition: all .3s ease-in-out;}
a{text-decoration:none;color:#444;}
a:hover{color:#166279;}
li{vertical-align:top;}
.fl{float:left;}
.fr{float:right;}
/*reset结束*/
/*arial字体*/
.ar{font-family:arial;}
/*本文件整合的是整站使用到的公共css样式*/
/*top*/
.top{width:100%;height: 30px;background: #F6F6F6;}
.top_box{width:1200px;height:30px;margin:0 auto;}
.top_box span{display:block;height:30px;line-height:30px;float: left;}
.top_box span.tool{min-width:50px;padding: 0 30px 0 0;line-height:30px;color:#fff;font-size:14px;}
.top_box span.tool a{color:#464646;margin-right:16px;float:left;}
.top_box span.tool a:hover{color:#166279;}
.top_box span.tool i{width: 20px; height:20px; float:left; background:red; margin:6px 10px 0 0; border-radius:20px;}
.top_box span.tool i img{width:20px; height:20px;}
.top_box span.tool2{float: right;}
.top_box span.tool2 a{margin-right:0;}
.top_box span.tool2 i{width:16px;height:16px;margin:7px 7px 0 0;}
.top_box span.tool2 i img{width:16px;height:16px;}
/*logo和搜索*/
.logoandsearch{height:130px;background:#Fff;}
.logoandsearch_box{width: 1200px;height:130px;margin:0 auto;}
.logoandsearch_box .logo{width:224px;height:52px;float: left;margin-top:39px;}
.logoandsearch_box .logo img{width:224px;height:52px;}
.logoandsearch_box .search{width:640px;padding-top:38px;float:left;margin-left:80px;}
.logoandsearch_box .search .input{width:638px;height: 40px;border:1px solid #FF4F03;background:#fff;position: relative;border-radius:3px;}
.logoandsearch_box .search .input .text{display: block;width:527px;height:40px;background:#fff;border: none;line-height:40px;padding:0 4px;font-size:13px;color:#666;position:absolute;left:3px;top:0px;}
.logoandsearch_box .search .submit{display: block; width:100px; height:40px; position:absolute; right:0px; top:0px; border:none; cursor: pointer; color:#FFF; font-size:15px; text-indent:26px; background:#FF4F03 url(../img/icon/search.jpg) no-repeat 15px 11px;}
.logoandsearch_box .search .submit:hover{background:#FF4000 url(../img/icon/search.jpg) no-repeat 16px 11px;}
.logoandsearch_box .search p{line-height:35px;font-size:13px;}
.logoandsearch_box .search p a{padding:0 3px;color:#777;}
.logoandsearch_box .search p a:hover{color:#000;}
.new_media{height:45px;width:210px;float: right;margin-top:38px;position: relative;z-index:100;}
.new_media a.me{display:block;width:48px;height:48px;float: left;margin-left:20px;position: relative;z-index:100;}
.new_media .bl{width:10px;height:9px;background:red;position:absolute;left:45px;top:-8px;background:url(../img/icon/bl.png) no-repeat center;}
.new_media a.me1{background:url(../img/icon/tr1.png) no-repeat center;}
.new_media a.me1:hover{background:url(../img/icon/tr11.png) no-repeat center;}
.new_media a.me2{background:url(../img/icon/tr2.png) no-repeat center;}
.new_media a.me2:hover{background:url(../img/icon/tr22.png) no-repeat center;}
.new_media a.me3{background:url(../img/icon/tr3.png) no-repeat center;}
.new_media a.me3:hover{background:url(../img/icon/tr33.png) no-repeat center;}
.new_media a .son{display:none;width: 98px;height: 100px;padding: 5px 0px 3px;text-align: center;background: #FFF none repeat scroll 0% 0%;position: absolute;top: 53px;left: -25px;z-index: 90;border:1px solid #D2D2D2;color:#FF4F03;}
.new_media a .son img{display: block;width: 75px;height: 75px;margin: 0px auto 2px;}
/*nav*/
.nav{width:100%;height:45px;background:#333333;}
.nav_box{width:1200px; height:45px; margin:0 auto;}
.nav_box li{float: left;width:150px;height:45px;text-align: center;line-height:45px;font-size:18px;color:#FFF;}
.nav_box li a{display: block;width:100%;height:100%;color:#fff;}
.nav_box li a:hover{background:#FF4F03;}
.nav_box .on{background:#FF4F03!important;}

