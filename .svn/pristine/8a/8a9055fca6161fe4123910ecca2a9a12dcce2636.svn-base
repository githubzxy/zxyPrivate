/**footer**/
.footer{height:200px;background:#fff;}
.flogo{width:1200px;margin:0 auto;height:120px;}
.ftlogo_div {width:400px;height:68px;border-right:1px solid #D4D4D4;float:left;margin-top:26px;}
.flogo img.ftlogo{display:block;margin-left:50px;width:300px;height:20px;margin-top:20px;}
.ft_icon{float:left;width:230px;padding:0 88px;border-right:1px solid #D4D4D4;height:68px;margin-top:26px;position:relative;}
.ft_icon a{display:block;width:48px;height:48px;float:left;margin:10px 14px 0 14px;}
.ft_icon a.qq{background:url(../images/icon/QQ_icon.png) no-repeat center;}
.ft_icon a.qq:hover{background:url(../images/icon/QQ_icon_pre.png) no-repeat center;}
.ft_icon a.wechat{background:url(../images/icon/wechat_icon.png) no-repeat center;}
.ft_icon a.wechat:hover{background:url(../images/icon/wechat_icon_pre.png) no-repeat center;}
.ft_icon a.tel{background:url(../images/icon/phone_icon.png) no-repeat center;}
.ft_icon a.tel:hover{background:url(../images/icon/phone_icon_pre.png) no-repeat center;}
.gwxLi{display:none;position:absolute;top:-5px;right:255px;z-index:8888;width:132px;height:120px;}
.gwxli_img{width:170px;height:165px;background: url(../img/b_icon.png) no-repeat 0px 0px ;z-index:9999;}
.servicetel{float:left;padding:0 0 0 88px ;width:328px;height:68px;line-height:68px;margin-top:26px;background:url(../img/icon/tel.png) no-repeat 58px 20px;border-right:1px solid #D4D4D4;}
.servicetel .telnum{height:50px;font-size:18px;color:#333;}
.servicetel .telnum span{text-align:left;font-size:12px;color:#656666;font-family: '微软雅黑';}
.friendshiplink {height:68px;float:left;margin-top:26px;line-height:68px;padding-left:40px;}
.friendshiplink a {color:#285b91;}
.copyright{width:100%;height:80px;background:#323232;}
.copyright .w1200{height:65px;padding-top:13px;overflow: hidden;text-align:center;}
.yqlink{height:20px;font-size:12px;}
.yqlink .bt{font-size:12px;color:#b8b8b8;}
.yqlink a{color:#b8b8b8;font-size:12px;line-height:12px; display:inline-block; padding:0 5px;border-right:1px solid #a7a7a7;}
.yqlink a:hover{color:#fff;}
.copyr {color: #a7a7a7;font-size: 12px;line-height: 20px;text-align: center;padding-top:13px;text-shadow: 1px 1px 1px #111;}
.link a{color:#a7a7a7; display:inline-block; padding:0 5px; border-right:1px solid #a7a7a7; height:12px; line-height:12px; margin:3px 0; }
.link .b_r,.yqlink .b_r{border-right:none;}
/**footer**/
/*标题*/
.txt_title{display: inline-block;float: left;height:100%;padding:0 0 0 20px;background:url(../img/icon/txt_title.jpg) no-repeat left center;font-size:18px;color:#333333;}
.more_link{display: inline-block;float: left;height:100%;padding:0 25px 0 0; color:#666; float: right; font-size:14px; margin-right:10px; background:url(../img/icon/link_more.png) no-repeat right center;}
.more_link:hover{color: #FF4E00;}
/*btn1*/
.btn1{width:100px;height:28px;display:block;line-height:28px;line-height:28px;text-align: center; color: #FF4F03; font-size:14px; border:1px solid #FF4F03; border-radius:3px;}
.btn1:hover{color: #FF7373;border:1px solid #FF7373;}
/*btn2*/
.btn2{width:100px;height:35px;display:block;line-height:35px;text-align: center; color: #fff; font-size:18px; border-radius:3px;background:#FE6700;}
.btn2:hover{background:red;color:#fff;}
/*main*/
.main{width:1200px;padding:20px 0;margin:0 auto;overflow: hidden;}
/*分页*/
.page2{width:100%;height:50px;line-height:50px;text-align:center;border-top:none;margin:0 0 10px 10px;}
.page2 a{padding:4px 8px !important;color:#666 !important;margin:0 6px;border:1px solid #888;background:#fff;border-radius:3px; box-shadow: 0px 0px 2px #CCC;}
.page2 a:hover{color:#AC130E;border-radius:3px; box-shadow: 0px 0px 7px #666;}
.page2 span{padding:4px 8px;color:#666;margin:0 6px;border:1px solid #888;background:#ccc;color:#fff;border-radius:3px; box-shadow: 0px 0px 2px #CCC;}
.page2 font{color:#000;}
.page2 .disabled{cursor: not-allowed;}
/*common_box*/
.common_box{border: 1px solid #E3E3E3; padding: 20px 15px; background: #FFF none repeat scroll 0% 0%; overflow: hidden;}