@charset "gb2312";
ul,li {
	margin:0;
	padding:0;
	border:0;
	list-style:none;
}
body, h1, h2, h3, h4, h5, h6, dl, dt, dd, ul, ol, li, th, td, p, blockquote, pre, form, fieldset, legend, input, button, textarea, hr {
    margin: 0;
    padding: 0;
}
img {border:0;}

body {
	background-color:#f7f7f7;
	font: 12px/1.5 Tahoma,Arial,"瀹��",sans-serif;
}
#container { padding:20px 0;}
.shell {
	width:100%;
	margin:0 auto;
}
.small-nav { color:#6f6636; padding-bottom:20px; }
.small-nav a{ color:#6f6636; text-decoration:none;}
.small-nav a:hover{
	color:#2d64b3;
	text-decoration:underline;
}
#main {}
#content {
	float:left;
	width:100%;
}
.box { background:#fbfcfc; height:100%; padding:1px; margin-bottom:20px; padding-bottom:15px;}
.box-head { background:#9097a9; height:32px; color:#fff; padding: 0 10px; line-height:32px; white-space:nowrap; border-bottom:solid 1px #fff; }
.box-head h2 { font-size:15px; font-weight: normal; }
.box-head .right{ padding-top:5px; line-height:13px;}
.box-head .right label,
.box-head .right input { float:left; }
.box-head .right label { padding:4px 0 0 0;}
.box-head .right input { margin-left:5px;}

.box-content { padding:10px; }

.box { -moz-border-radius:5px; -webkit-border-radius:5px; -moz-box-shadow: 0 0 6px #978f6c; -webkit-box-shadow: 0 0 6px #978f6c;}
.box-head { -moz-border-radius-topleft:5px; -moz-border-radius-topright:5px; -webkit-border-top-left-radius:5px; -webkit-border-top-right-radius:5px; }

.textarea{resize:none;}
.form .field { display:block;}
.field, .x-textbox, .xzyx{ -moz-border-radius:4px; -webkit-border-radius:4px;}
.field { border:solid 1px #d3cfc7; background:#fff; padding:5px;}
.chex, .chex_s {vertical-align:middle; margin-right:3px;}
.login-box{
	float:left;
	background:none repeat scroll 0 0 #DEDEDE;
	padding:5px;
	width:459px;
	margin-top: 40px;
}
.login-box-content {
    -moz-border-bottom-colors: none;
    -moz-border-image: none;
    -moz-border-left-colors: none;
    -moz-border-right-colors: none;
    -moz-border-top-colors: none;
    background: none repeat scroll 0 0 #FFFFFF;
    border-color: #CCCCCC;
    border-style: none solid solid;
    border-width: 0 1px 1px;
    padding: 0;
    width: 457px;
}
.login-box-content {
    border-style: none solid solid;
}
.login-tabul{
    background:#F2F2F2;
    border-bottom:1px solid #CCCCCC;
    color: #333333;
    height: 50px;
    line-height: 50px;
    text-align:left;
    width:440px;
	padding-left:17px;
	font-weight:700;
}
.x-form {
	margin-top: 20px;
}
.x-formfield:after {
    clear: both;
    content: ".";
    display: block;
    height: 0;
    visibility: hidden;
}
.x-formfield {
    line-height: 28px;
    margin-bottom: 10px;
}
.x-formfield span a {
	color:#2d64b3;
	text-decoration:none;
}
.x-formfield span a:hover {
	color:#2d64b3;
	text-decoration:none;
}
/*.x-formfield {
    line-height: 28px;
}*/
.x-formfield-container {
    margin-left: 90px;
}
.x-formfield-container span , .x-formfield span .error {
	color:#ba4c32;
}

.x-formfield-container span a{
	color:#2d64b3;
	text-decoration:none;
}
.x-formfield-container span a:hover {
	color:#32A2FF;
	text-decoration:underline;
}
.x-textbox {
    background: none repeat scroll 0 0 #FFFFFF;
    border: 1px solid #CCCCCC;
    color: #585858;
    font: 12px/20px Tahoma,Arial,"瀹��",sans-serif;
    height: 20px;
    padding: 4px 5px;
    vertical-align: middle;
    
}
.size1 { width:500px; height:150px; font-size:12px;}
.size4 { width:320px; }
.xzyx {
	color:#333;
	padding:4px 5px;
	background: none repeat scroll 0 0 #FFFFFF;
    border: 1px solid #CCCCCC;
	width: 219px;
	height:30px;
}
.x-textbox:focus, .xzyx:focus, .field:focus {
    border: 1px solid #857b42;
	color:#000;
}
.x-formfield-label {
	display: block;
	float: left;
	padding-right: 15px;
	text-align: right;
	width: 150px;
}

.x-button{
    background:url(../images/houtai/loginbtn.png) no-repeat;
	width:78px;
	height:32px;
	line-height:26px;
	text-align:center;
	color:#fff;
	font-weight:700;
	border:0;
	margin-right:5px;
	cursor:pointer;
}
